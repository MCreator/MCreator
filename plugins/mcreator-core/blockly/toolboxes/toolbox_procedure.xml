<xml id="toolbox">
    <custom-other/>
    <sep/>
    <category name="${t:blockly.category.minecraft_components}" colour="360">
        <block type="coord_x"/>
        <block type="coord_y"/>
        <block type="coord_z"/>
        <block type="mcitem_allblocks"/>
        <block type="mcitem_all"/>
        <block type="entity_from_deps"/>
        <block type="source_entity_from_deps"/>
        <block type="immediate_source_entity_from_deps"/>
        <block type="entity_iterator"/>
        <block type="damagesource_from_deps"/>
        <block type="direction_constant"/>
        <block type="direction_from_deps"/>
        <custom-mcelements/>
    </category>
    <sep/>
    <category name="${t:blockly.category.custom_variables}" colour="150">
        <custom-customvariables/>
    </category>
    <sep/>
    <category name="${t:blockly.category.logic_and_loops}" colour="120">
        <block type="controls_if"/>
        <block type="controls_if">
            <mutation else="1"/>
        </block>
        <block type="controls_if">
            <mutation elseif="1"/>
        </block>
        <block type="logic_ternary_op"/>
        <block type="controls_repeat_ext"/>
        <block type="controls_while"/>
        <block type="controls_flow_statements"/>
        <custom-logicloops/>
    </category>
    <category name="${t:blockly.category.logic}" colour="%{BKY_LOGIC_HUE}">
        <block type="logic_negate"/>
        <block type="logic_boolean"/>
        <block type="logic_binary_ops"/>
        <block type="math_binary_ops"/>
        <block type="text_binary_ops"/>
        <custom-logicoperations/>
    </category>
    <category name="${t:blockly.category.math}" colour="%{BKY_MATH_HUE}">
        <block type="math_number"/>
        <block type="math_java_constants"/>
        <block type="math_dual_ops"/>
        <block type="math_singular_ops"/>
        <block type="math_from_text"/>
        <custom-math/>
    </category>
    <category name="${t:blockly.category.text}" colour="%{BKY_TEXTS_HUE}">
        <block type="text"/>
        <block type="text_join"/>
        <block type="text_print"/>
        <block type="text_length"/>
        <block type="text_is_empty"/>
        <block type="text_replace"/>
        <block type="text_substring_from"/>
        <block type="text_substring"/>
        <block type="text_contains"/>
        <block type="text_starts_with"/>
        <block type="text_ends_with"/>
        <block type="text_trim"/>
        <block type="text_uppercase"/>
        <block type="text_lowercase"/>
        <block type="text_format_number">
            <value name="format">
                <block type="text">
                    <field name="TEXT">##.##</field>
                </block>
            </value>
        </block>
        <custom-text/>
    </category>
    <category name="${t:blockly.category.time}" colour="#628c94">
        <block type="time_seconds"/>
        <block type="time_minutes"/>
        <block type="time_hours"/>
        <block type="time_day_of_week"/>
        <block type="time_week_of_year"/>
        <block type="time_day_of_month"/>
        <block type="time_month"/>
        <block type="time_year"/>
        <block type="time_as_string"/>
        <block type="time_to_formatted_string">
            <value name="format">
                <block type="text">
                    <field name="TEXT">yyyy-MM-dd</field>
                </block>
            </value>
        </block>
        <custom-time/>
    </category>
    <sep/>
    <custom-apis/>
    <category name="${t:blockly.category.advanced}" colour="250">
        <block type="cancel_event"/>
        <block type="set_event_result"/>
        <block type="call_procedure">
            <mutation inputs="0"/>
        </block>
        <block type="call_procedure">
            <mutation inputs="3"/>
            <field name="name0">x</field>
            <value name="arg0">
                <block type="coord_x"/>
            </value>
            <field name="name1">y</field>
            <value name="arg1">
                <block type="coord_y"/>
            </value>
            <field name="name2">z</field>
            <value name="arg2">
                <block type="coord_z"/>
            </value>
        </block>
        <block type="java_code"/>
        <block type="java_code_get"/>
        <block type="debug_marker"/>
        <block type="entity_none"/>
        <custom-advanced/>
    </category>
</xml>
